{
  "name": "react-socrates",
  "version": "0.0.8",
  "description": "Simple and opinionated React & Socrates (Redux) boilerplate.",
  "scripts": {
    "build": "npm run clean-dist && webpack --config webpack.config.babel.js --colors",
    "dev": "webpack-dev-server --config webpack.config.babel.js --hot --inline --colors --history-api-fallback",
    "lint": "eslint ./; exit 0",
    "reset": "npm run clean && npm install",
    "clean": "npm run clean-npm && npm run clean-dist",
    "update": "updtr",
    "clean-dist": "rm -rf dist",
    "clean-npm": "rm -rf node_modules",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Bryan Fillmer <bryan.fillmer@gmail.com> (http://github.com/bfillmer)",
  "repository": {
    "type": "git",
    "url": "https://github.com/bfillmer/initial-reaction"
  },
  "license": "MIT",
  "dependencies": {
    "bootstrap": "^4.0.0-alpha.2",
    "classnames": "^2.2.5",
    "enroute": "^1.0.1",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "redux-routing": "^0.3.1"
  },
  "devDependencies": {
    "babel-core": "^6.7.4",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "css-loader": "^0.23.1",
    "eslint": "^2.4.0",
    "eslint-config-airbnb": "^7.0.0",
    "eslint-plugin-jsx-a11y": "^0.6.2",
    "eslint-plugin-react": "^4.2.3",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.17.0",
    "node-sass": "^3.7.0",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "updtr": "^0.1.14",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  }
}
